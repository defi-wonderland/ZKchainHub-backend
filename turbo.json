{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": [],
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["build", "test"],
      "outputs": []
    }
  }
}
