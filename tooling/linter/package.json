{
  "name": "@hyperhub/linter",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./base": "./base.js"
  },
  "scripts": {
    "format": "prettier --check .",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit"
